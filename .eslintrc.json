{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],

  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "quotes": ["error", "single", {"allowTemplateLiterals": true}],
    "func-names": "off",
    "strict": "off",
    "arrow-body-style": "off",
    "camelcase": "off"
  }
}

